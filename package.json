{
  "name": "webpack-experiments",
  "version": "1.0.0",
  "description": "Study webpack 4",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node",
    "node": "cross-env NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "serve": "cross-env NODE_ENV=development webpack serve --config webpack.config.js --mode development --open",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "author": "AlexShcherbackov",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
    "mini-css-extract-plugin": "^1.3.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  }
